        $(function () {
            //获取学校要闻
            $.ajax({
                url: "/News/GetXueXiaoYaoWen",
                type: "POST",
                async: true,
                data: {

                },
                success: function (resTxt) {
                    if (resTxt.length > 0) {
                        $("#xuexiaoyaowenhref").attr("href", "/More/Index?ID=" + resTxt[0].ID + "&type=" + resTxt[0].ClassID + "");
                        for (var i = 0; i < resTxt.length; i++) {
                            var time = ChangeDateFormat(resTxt[i].UpdateTime).toString().split('-');
                            if (resTxt[i].Title.length > 18)
                                $("#xuexiaoyaowen").append('<li><a href="/Wenhua/Detail?ID=' + resTxt[i].ID + '&type=' + resTxt[i].ClassID + '" class="left">' + resTxt[i].Title.substring(0, 25) + '</a><span class="right">' + time[2] + '/<em>' + time[1] + '/' + time[0] + '</em></span></li>');
                            else
                                $("#xuexiaoyaowen").append('<li><a href="/Wenhua/Detail?ID=' + resTxt[i].ID + '&type=' + resTxt[i].ClassID + '" class="left">' + resTxt[i].Title + '</a><span class="right">' + time[2] + '/<em>' + time[1] + '/' + time[0] + '</em></span></li>');
                        }
                    }
                }
            });
