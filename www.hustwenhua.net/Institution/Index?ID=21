        function getUrlParam(name) {
            var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)"); //构造一个含有目标参数的正则表达式对象
            var r = window.location.search.substr(1).match(reg);  //匹配目标参数
            if (r != null) return unescape(r[2]); return null; //返回参数值
        }

        function bumenjianjie()
        {
            var InstitutionID = "";
            if (getUrlParam("InstitutionID") != null)
                InstitutionID = getUrlParam("InstitutionID");
            else
                InstitutionID = getUrlParam("ID");
            location.href = "/Institution/More?type=1&InstitutionID="+InstitutionID;
        }

        function zhengcefagui() {
            var InstitutionID = "";
            if (getUrlParam("InstitutionID") != null)
                InstitutionID = getUrlParam("InstitutionID");
            else
                InstitutionID = getUrlParam("ID");
            location.href = "/Institution/More?type=2&InstitutionID=" + InstitutionID;
        }

        function gonggaotongzhi() {
            var InstitutionID = "";
            if (getUrlParam("InstitutionID") != null)
                InstitutionID = getUrlParam("InstitutionID");
            else
                InstitutionID = getUrlParam("ID");
            location.href = "/Institution/More?type=3&InstitutionID=" + InstitutionID;
        }

        function xinxifabu() {
            var InstitutionID = "";
            if (getUrlParam("InstitutionID") != null)
                InstitutionID = getUrlParam("InstitutionID");
            else
                InstitutionID = getUrlParam("ID");
            location.href = "/Institution/More?type=4&InstitutionID=" + InstitutionID;
        }

        function bumenshouye()
        {
            var InstitutionID = "";
            if (getUrlParam("InstitutionID") != null)
                InstitutionID = getUrlParam("InstitutionID");
            else
                InstitutionID = getUrlParam("ID");
            location.href = "/Institution/Index?ID=" + InstitutionID;
        }

        $(function () {
            var InstitutionID = "";
            if (getUrlParam("InstitutionID") != null)
                InstitutionID = getUrlParam("InstitutionID");
            else
                InstitutionID = getUrlParam("ID");
            $.ajax({
                url: "/All/GetVisitCount",
                type: "POST",
                data: {

                },
                success: function (resTxt) {
                    $("#totalVisit").html(resTxt);
                }
            });
        })

        
